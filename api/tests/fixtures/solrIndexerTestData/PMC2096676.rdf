<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096676/reference/B14">
	<bibo:citedBy rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096676/section/Broadening-the-Scope-of-Surveillance-and-Analysis/paragraph_1"/>
