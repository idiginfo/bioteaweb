<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/Background">
	<dcterms:hasPart rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/Background/paragraph_1"/>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/Background/paragraph_1">
	<dcterms:isPartOf rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/Background"/>
	<rdf:type rdf:resource="http://www.w3.org/2011/content#Content"/>
	<rdf:type rdf:resource="http://www.w3.org/2011/content#ContentAsText"/>
	<cnt:chars>Bioidentical Hormone Replacement Therapy (BHRT) describes supplementation of hormones that are molecularly identical to those hormones produced in the human body. Unlike manufactured Conventional Hormone Therapy (CHT), such as conjugated estrogens (CE) and medroxyprogesterone acetate (MPA), BHRT hormones do not contain extra structural moieties which may alter hormone receptor binding and function in the human body.</cnt:chars>
	<cnt:characterEncoding>ISO-8859-1</cnt:characterEncoding>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/Background/paragraph_2">
	<rdf:type rdf:resource="http://purl.org/spar/doco/Paragraph"/>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/Background">
