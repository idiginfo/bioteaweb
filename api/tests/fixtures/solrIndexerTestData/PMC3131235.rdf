<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/section/BHRT-Effectiveness/paragraph_3">
	<bibo:cites rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC3131235/reference/F6"/>
